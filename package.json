{
  "author": "BTZ Games",
  "description": "BTZ Games's videogames launcher app for easy managing your games.",
  "name": "electron-test",
  "productName": "BTZ Launcher",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "electron-forge start",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "ng build && electron .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build-installer": "electron-builder",
    "electron-package-win": "electron-packager ./ BTZLauncher --platform=win32 --arch=x64 --ignore=node_modules",
    "electron-package-linux": "electron-packager ./ BTZLauncher --platform=linux --arch=x64 --ignore=node_modules",
    "electron-package-mac": "electron-packager ./ BTZLauncher --platform=darwin --arch=x64 --ignore=node_modules",
    "electron-builder": "ng build --configuration production && electron-builder -w"
  },
  "build": {
    "appId": "electron-test",
    "compression": "maximum",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/favicon.ico",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "installerIcon": "src/favicon.ico",
      "uninstallerIcon": "src/favicon.ico",
      "uninstallDisplayName": "BTZ Launcher",
      "license": "license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^13.0.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^13.0.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "bootstrap": "^5.2.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "rxjs": "~7.5.0",
    "swiper": "^8.4.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.2",
    "@angular/cli": "~14.2.2",
    "@angular/compiler-cli": "^14.2.0",
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@types/jasmine": "~4.0.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-packager": "^17.1.1",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.7.2"
  }
}
